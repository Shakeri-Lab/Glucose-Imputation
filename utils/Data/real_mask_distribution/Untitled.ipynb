    "from PIL import Image\n",
    "import glob\n",
    "import os\n",
    "\n",
    "for img_path in glob.glob(\"night/*.png\"):\n",
    "    img = Image.open(img_path).convert(\"RGB\")\n",
    "    pdf_path = os.path.splitext(img_path)[0] + \".pdf\"\n",
    "    img.save(pdf_path)\n"
   ]
